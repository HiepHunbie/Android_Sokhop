<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="vt_bg_dialog">#80000000</color>
    <dimen name="vt_common_dimen_large">16dp</dimen>
    <dimen name="vt_common_dimen_normal">12dp</dimen>
    <dimen name="vt_common_dimen_small">8dp</dimen>
    <dimen name="vt_common_dimen_smallest">4dp</dimen>
    <dimen name="vt_common_dimen_xlarge">24dp</dimen>
    <dimen name="vt_common_dimen_xxlarge">32dp</dimen>
    <dimen name="vt_text_large">18sp</dimen>
    <dimen name="vt_text_normal">16sp</dimen>
    <dimen name="vt_text_small">12sp</dimen>
    <dimen name="vt_text_smallest">10sp</dimen>
    <dimen name="vt_text_xlarge">20sp</dimen>
    <dimen name="vt_text_xxlarge">24sp</dimen>
    <string name="cancel">キャンセル</string>
    <string name="close">閉じる</string>
    <string name="confirm">確認</string>
    <string name="done">完了</string>
    <string name="downloading">ダウンロード中…</string>
    <string name="error">エラー</string>
    <string name="loading">ローディング…</string>
    <string name="no">否</string>
