    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.

        -->
    </style>
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="TVTheme_Translucent_HasActionBar" parent="@style/VTTheme_Normal">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="VTDefaultActionBar" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:displayOptions">showTitle</item>
        <item name="android:background">#000000</item>
        <item name="android:titleTextStyle">@style/VTDefaultActionBarTitleText</item>
    </style>
    <style name="VTDefaultActionBarTitleText" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:textColor">#FFFFFF</item>
    </style>
    <style name="VTProgressDialogDefault" parent="android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
    </style>
    <style name="VTTheme_Dialog_FullscreenWithBackground" parent="@style/VTTheme_NoActionBar">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@color/vt_bg_dialog</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="VTTheme_NoActionBar" parent="@android:style/Theme.Holo.Light.DarkActionBar">
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">false</item>
    </style>
    <style name="VTTheme_Normal" parent="@android:style/Theme.Holo.Light.DarkActionBar">
        <item name="android:windowFullscreen">false</item>
        <item name="android:actionBarStyle">@style/VTDefaultActionBar</item>
    </style>
    <style name="VTTheme_Translucent" parent="@style/VTTheme_NoActionBar">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
</resources>